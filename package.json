  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build_client": "cd client; npm ci --production; npm run build",
    "build_server": "npm ci --produciton",
    "build": "npm build_client; npm build_server",
    "debug": "PORT=4000 nodemon --inspect ./bin/www",
    "client": "node start_client.js",
    "server": "node start_server.js",
    "dev": "concurrently --raw \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^0.25.0",
    "debug": "~2.6.9",
    "dotenv": "^14.3.2",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.13.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  }
}
